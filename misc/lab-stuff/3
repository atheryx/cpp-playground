#include <iostream>
#include <string.h>

using namespace std;

template<typename T>
int search(T *p, T key, int left, int right)
{
	if (left > right)
		return -1;

	int mid = (left + right) /2 ;
	if (p[mid] > key) {
		return (p, key, left, mid-1);
	} else if (p[mid] < key) {
		return (p, key, mid+1, right);	
	} else
		return mid;

	return -1;
}

int main()
{
	int arr[] = {1, 3, 5, 6, 9};

	cout << "Item found at: " << search(arr, 6, 0, strlen(arr)) << endl;

	return 0;
}
